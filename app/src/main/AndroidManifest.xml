        android:icon="@drawable/splashimage"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splashimage"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileDeviceInfoApp">
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.InternetActivity"></activity>
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.OrientationActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.RtvectorActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.LinearActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.GravityActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.gamerotationActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.GyroscopeActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.MagActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.acceleActivity" />
