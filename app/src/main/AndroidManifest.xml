<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deviceinfo.mobiledeviceinfoapp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileDeviceInfoApp">
        <activity android:name=".OrientationActivity"></activity>
        <activity android:name=".RtvectorActivity" />
        <activity android:name=".LinearActivity" />
        <activity android:name=".GravityActivity" />
        <activity android:name=".gamerotationActivity" />
        <activity android:name=".GyroscopeActivity" />
        <activity android:name=".MagActivity" />
        <activity android:name=".acceleActivity" />
        <activity android:name=".ConnectionActivity" />
        <activity android:name=".OsActivity" />
        <activity android:name=".ScreenActivity" />
        <activity android:name=".NetworkActivity" />
        <activity android:name=".BatteryActivity" />
        <activity android:name=".Processoractivity" />
        <activity android:name=".SensorActivity" />
        <activity android:name=".ChoiceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>