        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.ScreenActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.NetworkActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.BatteryActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.Processoractivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.SensorActivity" />
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.ChoiceActivity"></activity>
        <activity android:name="com.mobiledeviceinfo.mobiledeviceinfoapp.MainActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-7557609468922859~4793579684" />
